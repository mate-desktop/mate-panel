<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema id="org.mate.panel.applet.notification-area">
    <key name="use-only-one-line" type="b">
      <default>true</default>
      <summary>Display icons on one line only</summary>
      <description>If disabled, icons will display in a grid (if necessary) which can have mutliple rows/columns to fill up space.</description>
    </key>
    <key name="min-icon-size" type="i">
      <default>26</default>
      <summary>Minimum icon size</summary>
      <description>When icons display in a grid, this is the minimum size an icon can take.</description>
    </key>
  </schema>
</schemalist>
