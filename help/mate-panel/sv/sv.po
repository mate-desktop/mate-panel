msgid "@@image: 'figures/titlebar_window.png'; md5=b3624c3e2eeb934a16ff5689c8163f44"
msgstr "@@image: 'figures/titlebar_window.png'; md5=b3624c3e2eeb934a16ff5689c8163f44"

