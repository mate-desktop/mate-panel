#: C/clock.xml:422(para)
msgid "<guilabel>C</guilabel> (Celsius)"
msgstr "<guilabel>C</guilabel> (Celsius)"

#: C/clock.xml:425(para)
msgid "<guilabel>K</guilabel> (Kelvin)"
msgstr "<guilabel>K</guilabel> (Kelvin)"

